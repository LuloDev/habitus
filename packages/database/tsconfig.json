{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "ESNext",
		"target": "ES2022",
		"outDir": "./dist",
		"incremental": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"removeComments": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true
	}
}
